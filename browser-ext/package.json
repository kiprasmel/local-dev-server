{
	"name": "local-dev-server",
	"displayName": "Local dev server",
	"version": "0.0.0",
	"description": "A basic Plasmo extension.",
	"author": "Kipras Melnikovas (http://kipras.org) <kipras@kipras.org>",
	"packageManager": "yarn@1.22.11",
	"scripts": {
		"dev": "yarn plasmo dev",
		"build": "yarn plasmo build && yarn plasmo build --target=firefox-mv2"
	},
	"dependencies": {
		"plasmo": "0.47.0",
		"react": "18.2.0",
		"react-dom": "18.2.0"
	},
	"devDependencies": {
		"@trivago/prettier-plugin-sort-imports": "3.2.0",
		"@types/chrome": "0.0.190",
		"@types/node": "17.0.43",
		"@types/react": "18.0.12",
		"@types/react-dom": "18.0.5",
		"prettier": "2.7.0",
		"typescript": "4.7.4"
	},
	"manifest": {
		"host_permissions": [
			"https://*/*"
		],
		"permissions": []
	}
}
